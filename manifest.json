			"urlCheck": false
		},
		"usingComponents": true,
		"resizable": true,
		"frameset": true,
		"libVersion": "3.7.1",
		"requiredPrivateInfos": [
			"getLocation",
			"onLocationChange",
			"startLocationUpdateBackground"
		],
		"permission": {
			"scope.userLocation": {
				"desc": "你的位置信息将用于小程序位置接口的效果展示"
			}
		},
		"unipush": {
			"enable": true
		}
	},
	/* app平台配置 */
	"app": {
		"distribute": {
			/* android打包配置 */
			"modules": {
				"uni-payment": {
					"alipay": {},
					"wxpay": {
						"android": {},
						"ios": {
							"appid": "",
							"universalLink": ""
						}
					}
				},
				"uni-location": {
					"system": {},
					"tencent": {}
				},
				"uni-ad": {
					"gdt": {}
				},
				"uni-push": {},
				"uni-map": {
					"tencent": {}
				},
				"uni-verify": {},
				"uni-facialVerify": {}
			},
			"icons": {
				"ios": {
					"appstore": "package/icon1024.png"
				},
				"android": {
					"xxhdpi": "package/icon144.png",
					"xxxhdpi": "package/icon192.png"
				}
			}
		}
	},
	/* app-harmony平台配置 */
	"app-harmony": {
		"distribute": {
			"bundleName": "io.dcloud.uniappx",
			"modules": {
				"uni-payment": {
					"alipay": {},
					"wxpay": {}
				},
				"uni-location": {
					"system": {}
				},
				"uni-push": {},
				"uni-map": {
					"tencent": {}
				},
				"uni-verify": {},
				"uni-facialVerify": {},
