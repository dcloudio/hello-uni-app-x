				}
			},
			"icons": {
				"foreground": "package/icon1024.png",
				"background": "package/background.png"
			},
			"splashScreens": {
				"startWindowIcon": "package/icon1024.png"
			}
		}
	},
	/* web平台配置 */
	"web": {
		"darkmode": true,
		"router": {
			"base": "/web/",
			"mode": ""
