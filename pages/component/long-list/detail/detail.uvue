<template>
		<view class="article-content">
			<text>{{content}}</text>
		</view>
</template>

<script>

	export default {
		data() {
			return {
				content: "",
			}
		},
		onLoad(event) {
			this.content = event["content"] ?? "";
		},
		methods: {
		}
	}
</script>

<style>
	.article-content {
		padding: 0 30rpx;
		overflow: hidden;
		font-size: 30rpx;
		margin-bottom: 30rpx;
	}
</style>
